<?xml version='1.0' encoding='UTF-8'?>
<MorpheusModel version="4">
    <Description>
        <Details></Details>
        <Title>Chemotaxis</Title>
    </Description>
    <Space>
        <Lattice class="square">
            <Neighborhood>
                <Order>1</Order>
            </Neighborhood>
            <Size symbol="size" value="200, 200, 0"/>
        </Lattice>
        <SpaceSymbol symbol="space"/>
    </Space>
    <Time>
        <StartTime value="0"/>
        <StopTime value="100"/>
        <TimeSymbol symbol="time"/>
    </Time>
    <Global>
        <Field symbol="F" value="0.0" name="FGF">
            <Diffusion rate="600"/>
        </Field>
        <Field symbol="W" value="0.0" name="Wnt">
            <Diffusion rate="600"/>
        </Field>
        <Field symbol="C" value="0.0" name="CXCL12a">
            <Diffusion rate="20"/>
        </Field>
        <Constant symbol="deltaC" value="0.2" name="CXCL12a external decay"/>
        <System solver="adaptive45" time-step="1.0" name="PDE">
            <DiffEqn symbol-ref="C">
                <Expression>alpha - deltaC*C</Expression>
            </DiffEqn>
            <DiffEqn symbol-ref="F">
                <Expression>PF - deltaF*F</Expression>
            </DiffEqn>
            <DiffEqn symbol-ref="W">
                <Expression>PW-deltaW*W</Expression>
            </DiffEqn>
        </System>
        <Constant symbol="deltaW" value="0.15" name="Wnt external decay"/>
        <Constant symbol="deltaF" value="0.15" name="FGF external decay"/>
    </Global>
    <CellTypes>
        <CellType class="biological" name="cells">
            <Property symbol="FR" value="0.6"/>
            <Property symbol="WR" value="0.4"/>
            <Property symbol="CXCLR" value="0.3"/>
            <Mapper name="Ligand concentration around CXCL12aR">
                <Input value="C"/>
                <Output symbol-ref="CXCLR" mapping="average"/>
            </Mapper>
            <Mapper name="Ligand concentration around FGFR">
                <Input value="F"/>
                <Output symbol-ref="FR" mapping="average"/>
            </Mapper>
            <Mapper name="Ligand concentration around WntR">
                <Input value="W"/>
                <Output symbol-ref="WR" mapping="average"/>
            </Mapper>
            <Chemotaxis field="C" contact-inhibition="false" strength="s" name="WntR Leading cells migration" retraction="true"/>
            <Chemotaxis field="F" contact-inhibition="false" strength="μ + s" name="FGFR Trailing cells migration" retraction="true"/>
            <Property symbol="s" value="0.7" name="Force of chemotaxis"/>
            <Property symbol="μ" value="0.001" name="Drag co-efficient"/>
            <Property symbol="alpha" value="0.2" name="CXCL12a production"/>
            <Property symbol="PF" value="0.5" name="FGF Production"/>
            <Property symbol="PW" value="3.0" name="Wnt Production"/>
        </CellType>
        <CellType class="medium" name="medium">
            <Constant symbol="alpha" value="0.0"/>
            <Constant symbol="PF" value="0.0"/>
            <Constant symbol="PW" value="0.0"/>
        </CellType>
    </CellTypes>
    <CellPopulations>
        <Population size="0" type="cells">
            <!--    <Disabled>
        <InitCellObjects mode="order">
            <Arrangement repetitions="20, 5, 0" displacements="10, 10, 0">
                <Box size="10.0, 10.0, 0.0" origin="100.0, 25.0, 0.0"/>
            </Arrangement>
        </InitCellObjects>
    </Disabled>
-->
            <InitRectangle mode="regular" number-of-cells="0.003 * size.x * size.y">
                <Dimensions size="size.x, size.y, 0" origin="0.0, 0.0, 0.0"/>
            </InitRectangle>
        </Population>
    </CellPopulations>
    <Analysis>
        <Gnuplotter time-step="100" decorate="true">
            <Plot>
                <Field symbol-ref="C" isolines="5" min="0"/>
                <Cells value="CXCLR">
                    <ColorMap>
                        <Color value="2.0" color="blue"/>
                        <Color value="0" color="red"/>
                    </ColorMap>
                </Cells>
            </Plot>
            <Terminal name="png"/>
            <Plot>
                <Field symbol-ref="F" isolines="5" min="0"/>
                <Cells value="FR">
                    <ColorMap>
                        <Color value="2" color="green"/>
                        <Color value="0" color="red"/>
                    </ColorMap>
                </Cells>
            </Plot>
            <Plot>
                <Field symbol-ref="W" isolines="5" min="0"/>
                <Cells value="WR">
                    <ColorMap>
                        <Color value="2" color="red"/>
                        <Color value="0" color="red"/>
                    </ColorMap>
                </Cells>
            </Plot>
        </Gnuplotter>
        <DependencyGraph format="svg" exclude-plugins="Gnuplotter"/>
    </Analysis>
    <CPM>
        <Interaction/>
        <ShapeSurface scaling="norm">
            <Neighborhood>
                <Distance>2</Distance>
            </Neighborhood>
        </ShapeSurface>
    </CPM>
</MorpheusModel>
